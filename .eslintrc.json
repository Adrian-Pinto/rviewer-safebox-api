{
    "env": {
        "es2021": true,
        "node": true,
        "mocha": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
      "no-console": 0,
      "no-unused-expressions": 0,
      "import/extensions": 0
    }
}
